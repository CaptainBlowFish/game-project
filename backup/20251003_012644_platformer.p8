--player
function make_player(player_num, egg, x, y, level)
    player = {
        level = level or 0,
        player_num = player_num,
        x = x or 10,--left
        y = y or 80,--top
        dx = 0,--movement on the x axis
        dy = 1, --movement on the y axis
        max_dx = 2,
        max_dy = 10,
        current_jump_height = 0, --Will increase as the player jumps 
        max_jump_height = 24,
        fall_speed = .2,
        falling = true,
        can_jump = true,
        facing_left = false,
        runing_sprite_1 = false,
        current_sprite = 1,
        map_width = 127,
        map_height = 16,
        bread_collected = 0,
        alive = true,
        update_animations = true, --will halve the frame rate
        cam = {
            x = 0,
            y = level *128
        }
    }
    return player
end

function add_new_player(player_num)
    player_num = player_num or 1
    add(players,make_player(player_num))
end

function make_nonHostile(egg,x,y)
    player = {
        egg=egg or false,
        x = x or 10,--left
        y = y or 80,--top
        dx = 0,--movement on the x axis
        dy = 1, --movement on the y axis
        max_dx = .5,
        max_dy = 16,
        current_jump_height = 0, --Will increase as the player jumps 
        max_jump_height = 24,
        fall_speed = .4,
        falling = true,
        can_jump = true,
        facing_left = false,
        runing_sprite_1 = false,
        current_sprite = 1,
        map_width = 127,
        map_height = 16,
        bread_collected = 0,
        alive = true,
        update_animations = true, --will halve the frame rate
        cam = {
            x = 0,
            y = 0
        }
    }
    return player
end

