--main
--mr duck man!!

function _init()
    screen_size = 128
    players = {}
    eggs = {}
    map_animation_frames = 3
    add(players,make_player(0,false,10,80,1))
    --add_new_player()
    music(0,0,12)
end

function _update()
    foreach(players,update_player)
    for player=1,#players do
    end
    
    if #players<1 then
        reload(0x1000, 0x1000, 0x2000,"platformer.p8")
        _init()
    end
    foreach(eggs,update_nonhostile)
end

function _draw()
    cls()
    if #players>0 then
        draw(players[1],true)
    end
    for player=2,#players do
        draw(players[player])
    end
    sspr(0,32,40,16,0,9,160,64)
    foreach(eggs, draw)    
end

