--player
function make_player(player_num,level,x,y)
    player = {
        level = level or 0,
        player_num = player_num,
        x = x or 10,--left
        y = y or 60+ (level or 0) *screen_size,--top
        dx = 0,--movement on the x axis
        dy = 1, --movement on the y axis
        max_dx = 2,
        max_dy = 10,
        current_jump_height = 0, --Will increase as the player jumps 
        max_jump_height = 26,
        fall_speed = .2,
        falling = true,
        can_jump = true,
        facing_left = false,
        runing_sprite_1 = false,
        current_sprite = 1,
        map_width = 127,
        map_height = 32, --Measured in map 8x8 tiles 
        map_height = 32,
        bread_collected = 0,
        alive = true,
        update_animations = true, --will halve the frame rate 'on 2s'
        cam = {
            x = 0,
            y = (level or 0) *screen_size
        }
    }
    return player
end

function add_new_player(player_num)
    player_num = player_num or 1
    add(players,make_player(player_num))
end

