function make_non_hostile(egg,x,y)
    non_hostile = {
        egg=egg or false,
        x = x or 10,--left
        y = y or 80,--top
        dx = 0,--movement on the x axis
        dy = 1, --movement on the y axis
        max_dx = .01,
        max_dy = 16,
        current_jump_height = 0, --Will increase as the player jumps 
        max_jump_height = 36,
        fall_speed = .4,
        falling = true,
        can_jump = true,
        facing_left = false,
        runing_sprite_1 = false,
        current_sprite = 1,
        map_width = 127,
        map_height = 16,
        update_animations = true, --animate it on 2s
        alive = true
    }
    return non_hostile
end

